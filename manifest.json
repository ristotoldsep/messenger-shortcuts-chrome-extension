{
  "manifest_version": 3,
  "name": "Messenger Keyboard Shortcuts",
  "version": "1.0.0",
  "description": "Adds custom keyboard shortcuts to messenger.com",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://www.messenger.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
